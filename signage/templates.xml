    <template id="signage_list" name="Signage List">
        <t t-call="website.layout">
            <div id="wrap" class="container">
                <div class="oe_structure oe_empty" />
                <div class="row col-md-offset-2">
                    <h1>Signage</h1>
                    <ul t-foreach="signages" t-as="signage">
                        <li>
                            <span t-field="signage.name" />
<!--
                            <a t-att-href="'/signage/%s/edit' % signage.name">
                                <span t-field="signage.name" />
                            </a>
-->
                            <ul t-foreach="signage.area_ids" t-as="area">
                                <li>
                                    <span t-field="area.name" />
                                    Limit
                                    <span t-field="area.limit" />
                                    <ul>
                                        <t t-foreach="area.page_ids" t-as="page">
                                            <li>
                                                <a t-att-href="'/signage/%s/edit' % page.id">
                                                    <span t-field="page.name" />
                                                </a>
                                                Offset
                                                <span t-field="page.offset" />
                                            </li>
                                        </t>
                                        <li>
                                            <a t-att-href="'/signage/%s/%s/new' % (signage.name, area.name)">
                                                <span>New Page</span>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </t>
    </template>

    <template id="signage_overview" name="Signage Overview">
        <t t-call="website.layout">
            <div id="wrap" class="container">
                <div class="container">
                    <div class="row">
                        <div class="col-md-offset-2 mt32">
                            <ul t-foreach="signages" t-as="signage">
                                <li>
                                    <h2 t-field="signage.name" />
                                    <ul t-foreach="signage.area_ids" t-as="area">
                                        <li>
                                            <a t-att-href="'/signage/%s/%s?token=%s' % (signage.name, area.name, signage.token)" t-if="signage.token"><span t-field="area.name" /></a>
                                            <span t-if="signage.token">(<t t-esc="'http://%s/signage/%s/%s?token=%s' % (website.domain, signage.name, area.name, signage.token)" />)</span>
                                            <a t-att-href="'/signage/%s/%s' % (signage.name, area.name)" t-if="not signage.token"><span t-field="area.name" /></a>
                                            <ul>
                                                <t t-foreach="area.page_ids" t-as="page">
                                                    <li>
                                                        <span t-field="page.name" />
                                                    </li>
                                                </t>
                                            </ul>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

    <template id="website.signage_page_template" name="Page Template">
        <t t-call="signage.layout">
            <style>
                <link href="https://fonts.googleapis.com/css?family=PT+Sans+Narrow" rel="stylesheet" />
            </style>
            <style>
                html, body {
                    height: 100%;
                    margin: 0;
                }
